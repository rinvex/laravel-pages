{
    "name": "rinvex/laravel-pages",
    "description": "Rinvex Pages is an integral part for your Laravel content management system (CMS), it affords an easy, yet powerful way to create and manage pages with full control over their URLs, active status, titles, content, and other attributes.",
    "type": "library",
    "keywords": [
        "ajax",
        "async",
        "laravel",
        "rinvex",
        "pages",
        "cache",
        "page",
        "cms"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/laravel-pages/issues",
        "source": "https://github.com/rinvex/laravel-pages",
        "docs": "https://github.com/rinvex/laravel-pages/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/laravel-pages/contributors"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/console": "^9.0.0 || ^10.0.0",
        "illuminate/database": "^9.0.0 || ^10.0.0",
        "illuminate/routing": "^9.0.0 || ^10.0.0",
        "illuminate/support": "^9.0.0 || ^10.0.0",
        "illuminate/view": "^9.0.0 || ^10.0.0",
        "rinvex/laravel-support": "^6.0.0",
        "spatie/eloquent-sortable": "^4.0.0",
        "spatie/laravel-sluggable": "^3.3.0",
        "spatie/laravel-translatable": "^6.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.31.0 || ^0.32.0",
        "illuminate/container": "^9.0.0 || ^10.0.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Rinvex\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Pages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rinvex\\Pages\\Providers\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
